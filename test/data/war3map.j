    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=udg_DifficultyMultiplier
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_ClearingUnitGroup=GetRandomSubGroup(1, GetUnitsOfTypeIdAll('n003'))
        call ForGroupBJ(udg_ClearingUnitGroup, function Trig_Level_1_Fire_Func003Func002A)
        call DestroyGroup(udg_ClearingUnitGroup)
        call AddSpecialEffectLocBJ(GetUnitLoc(GetLastCreatedUnit()), "Abilities\\Spells\\Other\\Volcano\\VolcanoDeath.mdl")
        call DestroyEffectBJ(GetLastCreatedEffectBJ())
        call GroupAddUnitSimple(GetLastCreatedUnit(), udg_MinionsOfFire)
        set udg_ClearingUnitGroup=GetRandomSubGroup(1, udg_ActiveCrabs)
        call ForGroupBJ(udg_ClearingUnitGroup, function Trig_Level_1_Fire_Func003Func009A)
        call DestroyGroup(udg_ClearingUnitGroup)
        call IssuePointOrderLocBJ(GetLastCreatedUnit(), "attack", GetUnitLoc(udg_Target))
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
endfunction

