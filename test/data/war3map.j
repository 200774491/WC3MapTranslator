//===========================================================================
// Trigger: Stack Modification
//===========================================================================
function Trig_Stack_Modification_Conditions takes nothing returns boolean
    if ( not ( GetItemCharges(GetManipulatedItem()) != 0 ) ) then
        return false
    endif
    return true
endfunction

function Trig_Stack_Modification_Func002Func001Func001C takes nothing returns boolean
    if ( not ( ( GetItemCharges(UnitItemInSlotBJ(GetManipulatingUnit(), GetForLoopIndexA())) + GetItemCharges(GetManipulatedItem()) ) > GetItemLevel(GetManipulatedItem()) ) ) then
        return false
    endif
    return true
endfunction

function Trig_Stack_Modification_Func002Func001C takes nothing returns boolean
    if ( not ( GetItemTypeId(UnitItemInSlotBJ(GetManipulatingUnit(), GetForLoopIndexA())) == GetItemTypeId(GetManipulatedItem()) ) ) then
        return false
    endif
    if ( not ( GetItemCharges(UnitItemInSlotBJ(GetManipulatingUnit(), GetForLoopIndexA())) < GetItemLevel(UnitItemInSlotBJ(GetManipulatingUnit(), GetForLoopIndexA())) ) ) then
        return false
    endif
    if ( not ( UnitItemInSlotBJ(GetManipulatingUnit(), GetForLoopIndexA()) != GetManipulatedItem() ) ) then
        return false
    endif
    return true
endfunction

function Trig_Stack_Modification_Actions takes nothing returns nothing
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=6
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( Trig_Stack_Modification_Func002Func001C() ) then
            if ( Trig_Stack_Modification_Func002Func001Func001C() ) then
                call SetItemCharges(GetManipulatedItem(), ( ( GetItemCharges(UnitItemInSlotBJ(GetManipulatingUnit(), GetForLoopIndexA())) + GetItemCharges(GetManipulatedItem()) ) - GetItemLevel(GetManipulatedItem()) ))
                call SetItemCharges(UnitItemInSlotBJ(GetManipulatingUnit(), GetForLoopIndexA()), GetItemLevel(GetManipulatedItem()))
            else
                call SetItemCharges(UnitItemInSlotBJ(GetManipulatingUnit(), GetForLoopIndexA()), ( GetItemCharges(UnitItemInSlotBJ(GetManipulatingUnit(), GetForLoopIndexA())) + GetItemCharges(GetManipulatedItem()) ))
                call RemoveItem(GetManipulatedItem())
            endif
        else
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
endfunction

