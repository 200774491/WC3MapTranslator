{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-empty": false,
        "no-bitwise": false,
        "object-literal-sort-keys": false,
        "variable-name": false,
        "align": [
            false,
            "parameters"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-line-length": [
            false
        ],
        "no-console": [
            false,
            "log",
            "error"
        ],
        "no-var-requires": false,
        "quotemark": [
            true,
            "single"
        ],
        "one-variable-per-declaration": false,
        "curly": [
            true,
            "ignore-same-line"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ]
    },
    "rulesDirectory": []
}